{
  "name": "gym-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "emulator:start": "npx firebase emulators:start --project=default-project --import=./emulator-data --export-on-exit=./emulator-data",
    "emulator:export": "npx firebase emulators:export ./emulator-data",
  "emulator:import": "npx firebase emulators:start --import=./emulator-data",
  "kill-emulator-8080": "pwsh -NoProfile -File scripts/kill-emulator.ps1",
    "gym-admin:serve": "cd projects/gym-admin && npm run start",
    "gym-admin:build": "cd projects/gym-admin && npm run build",
    "gym-admin:test": "cd projects/gym-admin && npm run test",
    "gym-app:serve": "cd projects/gym-app && ionic serve",
    "gym-app:build": "cd projects/gym-app && ionic build",
    "gym-app:android": "cd projects/gym-app && ionic cap run android",
    "gym-app:sync": "cd projects/gym-app && ionic cap sync",
    "library:build": "ng build gym-library",
    "library:watch": "ng build gym-library --watch",
    "library:test": "ng test gym-library",
    "dev": "npm run library:build && npm run dev:parallel",
    "dev:parallel": "concurrently \"npm run gym-admin:serve\" \"npm run gym-app:serve\" --names \"admin,app\" --prefix-colors \"cyan,magenta\"",
    "dev:all": "concurrently \"npm run library:watch\" \"npm run gym-admin:serve\" \"npm run gym-app:serve\" --names \"lib,admin,app\" --prefix-colors \"yellow,cyan,magenta\"",
    "dev:with-emulator": "npm run library:build && concurrently \"npm run emulator:start\" \"npm run gym-admin:serve\" \"npm run gym-app:serve\" --names \"emulator,admin,app\" --prefix-colors \"green,cyan,magenta\"",
    "dev:with-firebase": "npm run library:build && concurrently \"npm run gym-admin:serve:firebase\" \"npm run gym-app:serve:firebase\" --names \"admin,app\" --prefix-colors \"cyan,magenta\"",
    "gym-admin:serve:firebase": "cd projects/gym-admin && npm run start -- --configuration=firebase",
    "gym-app:serve:firebase": "cd projects/gym-app && ionic serve -- --configuration=firebase",
    "install:all": "npm install && cd projects/gym-admin && npm install && cd ../gym-app && npm install && cd ../gym-library && npm install"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.3",
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.2.1",
    "firebase-tools": "^14.19.1",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "typescript": "~5.8.2"
  }
}
